<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user_path(user), class:"btn btn-primary" %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path, class:"btn btn-primary" %>
<%= link_to "List of breweries", breweries_path, class:"btn btn-primary" %>
